{
  "build": {
    "env": {
      "SPACEID": "@spaceid",
      "TOKEN": "@token",
      "GATSBY_ALGOLIA_APP_ID": "@GATSBY_ALGOLIA_APP_ID",
      "GATSBY_ALGOLIA_INDEX_NAME": "@GATSBY_ALGOLIA_INDEX_NAME",
      "GATSBY_ALGOLIA_SEARCH_KEY": "@GATSBY_ALGOLIA_SEARCH_KEY",
      "GATSBY_ALGOLIA_ADMIN_KEY": "@GATSBY_ALGOLIA_ADMIN_KEY"
    }
  },
  "version": 2,
  "routes": [
    { "src": "/(.*)", "dest": "/$1" },
    { "handle": "filesystem" },
    { "src": "/.*", "status": 404, "dest": "/404" }
  ]
}

{
  "build": {
    "env": {
      "SPACEID": "@spaceid",
      "TOKEN": "@token",
      "GATSBY_ALGOLIA_APP_ID": "@gatsby_algolia_app_id",
      "GATSBY_ALGOLIA_INDEX_NAME": "@gatsby_algolia_index_name",
      "GATSBY_ALGOLIA_SEARCH_KEY": "@gatsby_algolia_search_key",
      "GATSBY_ALGOLIA_ADMIN_KEY": "@gatsby_algolia_admin_key"
    }
  },
  "version": 2,
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    },
    {
      "src": "/blog/download-gatsby-blog-starters-and-contentful-cms-template",
      "dest": "/blog/migrasi-blog-ke-gatsby",
      "status": 301
    },
    {
      "src": "/blog/orang-padang-vs-orang-minangkabau",
      "dest": "/blog/fakta-unik-minangkabau",
      "status": 301
    },
    {
      "src": "/blog/t-e-r-c-o-l-o-n-g/",
      "dest": "/blog/tercolong",
      "status": 301
    },
    {
      "src": "/blog/acid-pada-database-consistency",
      "dest": "/blog/acid-pada-database#consistency",
      "status": 301
    },
    {
      "src": "/blog/acid-pada-database-atomicity",
      "dest": "/blog/acid-pada-database#atomicity",
      "status": 301
    },
    {
      "src": "/blog/acid-pada-database-durability",
      "dest": "/blog/acid-pada-database#durability",
      "status": 301
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/.*",
      "status": 404,
      "dest": "/404"
    }
  ]
}
